package broker;

public class Offer[label Auction]
  where provider <= Auction
{
  public final int{Auction}@{Auction;⊥←} price; // just price
  public final int{Auction}@{Auction;⊥←} quality; // 0 = economy, 1 = business, 2 = first

  public Offer{Auction}(int{Auction} price, int{Auction} quality) {
    this.price = price;
    this.quality = quality;
    super();
  }
}

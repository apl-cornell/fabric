package fabric.worker.metrics.treaties;

import fabric.lang.JavaInlineable;
import java.io.Serializable;

/**
 * An inlineable reference to a treaty. Represented as the oid of the object
 * it's defined on and the unique id for treaties on that object.
 */
public final class TreatyRef implements Serializable, JavaInlineable {

  public static native TreatyRef createRef(MetricTreaty treaty);

  public native MetricTreaty get();
}
